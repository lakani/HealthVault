@using HealthVault.Entity.Model
@using HealthVault.Shared.DataTables

<MatSelect Label=@Label @bind-Value="@value">
    @if (Items != null)
    {
        @foreach (var item in Items)
        {
            <MatOptionString TValue="int" Value=@item.Identifier.ToString()>@item.value</MatOptionString>
        }
    }

</MatSelect>

@code {

    [Parameter]
    public short? value { get; set; }

    [Parameter]
    public String Label { get; set; }

    [Parameter]
    public IEnumerable<lut_general_usages> Items { get; set; }
}
